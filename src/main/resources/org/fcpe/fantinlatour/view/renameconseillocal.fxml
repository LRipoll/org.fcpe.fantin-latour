<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import org.fcpe.fantinlatour.model.*?>
<GridPane id="windows" prefHeight="100.0" prefWidth="300.0"
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="org.fcpe.fantinlatour.app.controller.RenameConseilLocalEtablissementController"
	alignment="center" hgap="10" vgap="10">
	<columnConstraints>
		<ColumnConstraints minWidth="150" prefWidth="150"
			maxWidth="Infinity" halignment="LEFT">
		</ColumnConstraints>
		<ColumnConstraints minWidth="150" prefWidth="150"
			maxWidth="Infinity" hgrow="ALWAYS">
		</ColumnConstraints>
	</columnConstraints>

	<Label fx:id="nameLabel"
		text="%org.fcpe.fantinlatour.view.renameconseillocal.name.label"
		GridPane.rowIndex="0" GridPane.columnIndex="0"/>
	<TextField fx:id="nameTextField"
		text="%org.fcpe.fantinlatour.view.renameconseillocal.name.text"
		GridPane.rowIndex="0" GridPane.columnIndex="1" />
	<Button fx:id="cancelButton" onAction="#cancel"
		text="%org.fcpe.fantinlatour.view.renameconseillocal.cancelButton.text"
		prefWidth="90" GridPane.columnIndex="0" GridPane.rowIndex="1" GridPane.halignment="CENTER">
	
		<tooltip>
			<Tooltip
				text="%org.fcpe.fantinlatour.view.renameconseillocal.cancelButton.tooltip.text" />
		</tooltip>
	</Button>
	<Button fx:id="okButton" onAction="#execute"
		text="%org.fcpe.fantinlatour.view.renameconseillocal.createButton.text"
		prefWidth="90"  GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.halignment="CENTER">
		<tooltip>
			<Tooltip
				text="%org.fcpe.fantinlatour.view.renameconseillocal.createButton.tooltip.text" />
		</tooltip>
	</Button>

</GridPane>